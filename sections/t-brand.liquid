{{ 'https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css' | stylesheet_tag}}
{{ 't-brand.css' | asset_url | stylesheet_tag}}

<div class="container">
    <div class="list-brand d-flex justify-content-xxl-between justify-content-center align-items-center">
      <div class="text-center">
        {{- 'versace-logo.svg' | inline_asset_content -}}
        {% comment %} <img src="./image/logo/versace-logo.svg" alt="versace"> {% endcomment %}
      </div>
      <div class="text-center">
        {{- 'zara-logo.svg' | inline_asset_content -}} 
      </div>
      <div class="text-center">
        {{- 'gucci-logo.svg' | inline_asset_content -}}
      </div>
      <div class="text-center">
        {{- 'prada-logo.svg' | inline_asset_content -}}
      </div>
      <div class="text-center">
        {{- 'calvin-klein-logo.svg' | inline_asset_content -}}
      </div>
    </div>
    <div class="list-brand-small">
      <div class="list-brand-small-item d-flex justify-content-between flex-nowrap">
        <div class="text-center">
            {{- 'versace-logo.svg' | inline_asset_content -}}
        </div>
        <div class="text-center">
            {{- 'zara-logo.svg' | inline_asset_content -}}
 
        </div>
        <div class="text-center">
            {{- 'gucci-logo.svg' | inline_asset_content -}}
        </div>
      </div>
      <div class="list-brand-small-item d-flex justify-content-between flex-nowrap">
        <div></div>
        <div class="text-center">
            {{- 'prada-logo.svg' | inline_asset_content -}}
        </div>
        <div class="text-center">
            {{- 'calvin-klein-logo.svg' | inline_asset_content -}}
        </div>
        <div></div>
      </div>
    </div>
  </div>

